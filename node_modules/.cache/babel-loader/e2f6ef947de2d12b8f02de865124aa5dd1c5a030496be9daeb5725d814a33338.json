{"ast":null,"code":"import{createReducer}from\"@reduxjs/toolkit\";export var cartReducer=createReducer({cartItems:[],subTotal:0,shipping:0,tax:0,total:0},{addToCart:function addToCart(state,action){var item=action.payload;var isItemExist=state.cartItems.find(function(i){return i.id===item.id;});if(isItemExist){state.cartItems.forEach(function(i){if(i.id===item.id)i.quantity+=1;});}else{state.cartItems.push(item);}},decrement:function decrement(state,action){var item=state.cartItems.find(function(i){return i.id===action.payload;});if(item.quantity>1){state.cartItems.forEach(function(i){if(i.id===item.id)i.quantity-=1;});}},deleteFromCart:function deleteFromCart(state,action){state.cartItems=state.cartItems.filter(function(i){return i.id!==action.payload;});},calculatePrice:function calculatePrice(state){var sum=0;state.cartItems.forEach(function(i){return sum+=i.price*i.quantity;});state.subTotal=sum;state.shipping=state.subTotal>1000?0:200;state.tax=+(state.subTotal*0.18).toFixed();state.total=state.subTotal+state.shipping+state.tax;console.log(state.total);}});","map":{"version":3,"names":["createReducer","cartReducer","cartItems","subTotal","shipping","tax","total","addToCart","state","action","item","payload","isItemExist","find","i","id","forEach","quantity","push","decrement","deleteFromCart","filter","calculatePrice","sum","price","toFixed","console","log"],"sources":["C:/Users/HP/Desktop/my-app/src/redux/reducers.js"],"sourcesContent":["import { createReducer } from \"@reduxjs/toolkit\";\n\nexport const cartReducer = createReducer({\n    cartItems:[],\n    subTotal:0,\n    shipping:0,\n    tax:0, \n    total:0\n},{\n    addToCart: (state, action)=>{\n        const item= action.payload;\n        const isItemExist = state.cartItems.find((i)=>i.id===item.id)\n\n        if(isItemExist){\n            state.cartItems.forEach((i)=>{\n                if(i.id===item.id) i.quantity +=1;\n            })\n        }else{\n            state.cartItems.push(item);\n        }\n    },\n    decrement:(state, action )=>{\n        const item = state.cartItems.find((i)=>i.id===action.payload);\n        if(item.quantity>1){\n            state.cartItems.forEach((i)=>{\n                if(i.id===item.id) i.quantity-=1;\n            })\n        }\n        \n    },\n    deleteFromCart:(state,action)=>{\n        state.cartItems= state.cartItems.filter(i=>i.id!==action.payload)\n    },\n    calculatePrice:(state)=>{\n       let sum=0;\n       state.cartItems.forEach((i)=>(sum+=(i.price*i.quantity)));\n       state.subTotal=sum;\n       state.shipping = state.subTotal > 1000 ? 0 : 200;\n       state.tax= +(state.subTotal*0.18).toFixed();\n       state.total=state.subTotal+state.shipping+state.tax;\n       console.log(state.total)\n\n\n    }\n});\n"],"mappings":"AAAA,OAASA,aAAa,KAAQ,kBAAkB,CAEhD,MAAO,IAAM,CAAAC,WAAW,CAAGD,aAAa,CAAC,CACrCE,SAAS,CAAC,EAAE,CACZC,QAAQ,CAAC,CAAC,CACVC,QAAQ,CAAC,CAAC,CACVC,GAAG,CAAC,CAAC,CACLC,KAAK,CAAC,CACV,CAAC,CAAC,CACEC,SAAS,CAAE,SAAAA,UAACC,KAAK,CAAEC,MAAM,CAAG,CACxB,GAAM,CAAAC,IAAI,CAAED,MAAM,CAACE,OAAO,CAC1B,GAAM,CAAAC,WAAW,CAAGJ,KAAK,CAACN,SAAS,CAACW,IAAI,CAAC,SAACC,CAAC,QAAG,CAAAA,CAAC,CAACC,EAAE,GAAGL,IAAI,CAACK,EAAE,GAAC,CAE7D,GAAGH,WAAW,CAAC,CACXJ,KAAK,CAACN,SAAS,CAACc,OAAO,CAAC,SAACF,CAAC,CAAG,CACzB,GAAGA,CAAC,CAACC,EAAE,GAAGL,IAAI,CAACK,EAAE,CAAED,CAAC,CAACG,QAAQ,EAAG,CAAC,CACrC,CAAC,CAAC,CACN,CAAC,IAAI,CACDT,KAAK,CAACN,SAAS,CAACgB,IAAI,CAACR,IAAI,CAAC,CAC9B,CACJ,CAAC,CACDS,SAAS,CAAC,SAAAA,UAACX,KAAK,CAAEC,MAAM,CAAI,CACxB,GAAM,CAAAC,IAAI,CAAGF,KAAK,CAACN,SAAS,CAACW,IAAI,CAAC,SAACC,CAAC,QAAG,CAAAA,CAAC,CAACC,EAAE,GAAGN,MAAM,CAACE,OAAO,GAAC,CAC7D,GAAGD,IAAI,CAACO,QAAQ,CAAC,CAAC,CAAC,CACfT,KAAK,CAACN,SAAS,CAACc,OAAO,CAAC,SAACF,CAAC,CAAG,CACzB,GAAGA,CAAC,CAACC,EAAE,GAAGL,IAAI,CAACK,EAAE,CAAED,CAAC,CAACG,QAAQ,EAAE,CAAC,CACpC,CAAC,CAAC,CACN,CAEJ,CAAC,CACDG,cAAc,CAAC,SAAAA,eAACZ,KAAK,CAACC,MAAM,CAAG,CAC3BD,KAAK,CAACN,SAAS,CAAEM,KAAK,CAACN,SAAS,CAACmB,MAAM,CAAC,SAAAP,CAAC,QAAE,CAAAA,CAAC,CAACC,EAAE,GAAGN,MAAM,CAACE,O